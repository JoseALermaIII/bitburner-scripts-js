{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Docs in Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": " ${workspaceFolder}\\..\\bitburner-scripts-js-docs\\html\\index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "pwa-node",
            "name": "Run Current File",
            "request": "launch",
            "program": "${workspaceFolder}\\src\\${file}"
        },
        {
            "type": "node-terminal",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        }
    ]
}